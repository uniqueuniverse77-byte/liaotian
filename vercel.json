{
  "rewrites": [
    {
      "source": "/pages/:slug",
      "destination": "/pages/:slug.md"
    },
    {
      "source": "/:slug",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/index.html",
      "headers": [
        {
          "key": "Content-Type",
          "value": "text/html; charset=utf-8"
        }
      ]
    }
  ],
  "cleanUrls": true
}
